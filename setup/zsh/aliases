# Config file dedicated to the user's personal aliases shared between all exegol containers

# PATH
alias opt='cd /opt'
alias ws='cd /workspace'
alias wsu='cd /workspace/uploads'
alias wsd='cd /workspace/downloads'
alias wso='cd /workspace/outputs'

# QUICK ACCESS
alias server='python3 /opt/my-resources/tools/put_server/server.py -p 80 -d /workspace/uploads'

alias get_tool='cd /opt && cp `fzf-wordlists` /workspace/uploads && cd /workspace/uploads'

# ENCODING
alias urlencode='python3 -c "import urllib.parse,sys; print(urllib.parse.quote(sys.argv[1]))"'
alias urldecode='python3 -c "import urllib.parse,sys; print(urllib.parse.unquote(sys.argv[1]))"'
